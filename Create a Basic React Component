import React, { useState } from 'react';
import './App.css';

function App() {
  const [name, setName] = useState("Anshul");
  const [count, setCount] = useState(0);

  return (
    <div className="App">
      <h1>Hello, {name}!</h1>
      <p>Button Clicked {count} times</p>
      <button onClick={() => setCount(count + 1)}>Click Me</button>
    </div>
  );
}

export default App;
